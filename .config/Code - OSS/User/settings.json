{
  "editor.find.cursorMoveOnType": false,
  "editor.find.loop": false,
  "editor.fontFamily": "cascadia code",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "diffEditor.wordWrap": "on",
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "debug.allowBreakpointsEverywhere": true,
  "debug.autoExpandLazyVariables": true,
  "debug.confirmOnExit": "always",
  "debug.console.acceptSuggestionOnEnter": "on",
  "gruvboxMaterial.darkContrast": "soft",
  "gulp.autoDetect": "on",
  "html.format.indentInnerHtml": true,
  "html.format.templating": true,
  "prettier.bracketSameLine": true,
  "workbench.layoutControl.type": "menu",
  "explorer.compactFolders": false,
  "explorer.decorations.badges": false,
  "explorer.incrementalNaming": "smart",
  "explorer.openEditors.sortOrder": "alphabetical",
  "explorer.openEditors.visible": 8,
  "files.exclude": {
    "LICENSE": true
  },
  "vim.camelCaseMotion.enable": true,
  "editor.scrollbar.horizontalScrollbarSize": 6,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[module.css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "workbench.startupEditor": "none",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.menuBarVisibility": "toggle",
  "sqltools.useNodeRuntime": true,
  "bracketPairColorizer.depreciation-notice": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "$HOME/.config/nvim/init.lua",
  "diffEditor.ignoreTrimWhitespace": false,
  "liveServer.settings.CustomBrowser": "firefox",
  "liveServer.settings.root": "public",
  "liveServer.settings.ignoreFiles": [".vscode/**"],
  "editor.formatOnSave": true,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.confirmSync": false,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "tabnine.experimentalAutoImports": true,
  "debug.console.fontSize": 16,
  "editor.fontSize": 16,
  "editor.codeLensFontSize": 16,
  "terminal.integrated.fontFamily": "cascadia code",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}

/* EXTENSIONS
   Auto Rename Tag
   BRACKET_PAIR
   DEBUGGER FOR CHROME
   ES7+REACT/REDUX/....
   ESLINT
   EVERFOREST
   GRUVBOX theme
   LIVE_SERVER
   PATH INTELLISENSE
   PRETTIER
   NEOVIM
  */
